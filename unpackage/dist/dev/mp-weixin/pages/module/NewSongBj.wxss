
.gradient.data-v-bcedbbcf {
		width: 100%;
		height: 160px;
		background-size: 600% 600% !important;
		animation: gradientBG-bcedbbcf 15s ease infinite;
		position: relative;
}
@keyframes gradientBG-bcedbbcf {
0% {
			background-position: 0% 50%;
}
50% {
			background-position: 100% 50%;
}
100% {
			background-position: 0% 50%;
}
}
.bj.data-v-bcedbbcf {
		width: 100%;
		height: 100%;
		position: absolute;
		top: 0;
		left: 0;
		background-color: rgba(0, 0, 0, 0.2);
}
